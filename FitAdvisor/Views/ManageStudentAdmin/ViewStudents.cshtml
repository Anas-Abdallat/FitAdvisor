@model FitAdvisor.Models.STUDENT

<h2>Instructor Details</h2>

<table class="table">
    <tr>
        <th>ID</th>
        <td>@Model.ID</td>
    </tr>
    <tr>
        <th>Full Name</th>
        <td>@Model.FULL_NAME</td>
    </tr>
    <tr>
        <th>Department</th>
        <td>@Model.DEPARTMENT</td>
    </tr>
    <tr>
        <th>Student Number</th>
        <td>@Model.STUDENT_NUMBER</td>
    </tr>
    <tr>
        <th>Email</th>
        <td>@Model.EMAIL</td>
    </tr>
</table>

<p>
    @Html.ActionLink("Back to List", "Index")
</p>
