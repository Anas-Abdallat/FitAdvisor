@model FitAdvisor.Models.STUDENT

<h2>Deactivate Studnt</h2>

<p>Are you sure you want to deactivate the Student <strong>@Model.FULL_NAME</strong>?</p>

@using (Html.BeginForm("DeactivateConfirmed", "ManageStudentAdmin", new { id = Model.ID }))
{
    @Html.AntiForgeryToken()
    <button type="submit" class="btn btn-danger">Confirm Deactivation</button>
}

<p>
    @Html.ActionLink("Cancel", "Index")
</p>
